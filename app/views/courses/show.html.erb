<h2>Course <%= @course.id %></h2>

<ul id="details">
  <li>Course Name: <%= @course.courseName %></li>
  <li>Description: <%= @course.courseDesc %></li>
</ul>

<%= button_to 'Delete', course_path(@course), :method => :delete, :confirm => 'Are you sure?' %>
<%=link_to 'Back to list of courses', courses_path %> 