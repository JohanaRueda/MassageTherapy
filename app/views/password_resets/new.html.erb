<% provide(:title, "Se olvidó password") %>

<h1>Se olvidó password</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag password_resets_path do %>
      <%= label_tag :email %>
      <%= text_field_tag :email%> <br>
      <%= submit_tag "Enviar", class: "btn btn-primary" %>
    <% end %>
    
  </div>
</div>