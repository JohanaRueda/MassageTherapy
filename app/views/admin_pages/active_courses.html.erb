<!DOCTYPE html>
<html>
<head></head>
<body>
    
<div class="tab">
  <button class="tablinks active"><%= link_to "Active Courses", admin_active_path, class: "prettyLink" %></button>
  <button class="tablinks"><%= link_to "Add Course", admin_add_path, class: "prettyLink" %></button>
  <button class="tablinks"><%= link_to "Edit Course", admin_edit_path, class: "prettyLink" %></button>
  <button class="tablinks"><%= link_to "Delete Course", admin_delete_path, class: "prettyLink" %></button>
</div>

<div id="Active" class="tabcontent">
  <table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  
</table>
  
  
  <h3>Active Tab</h3>
  <p>This is the first tab.</p>
  <% if logged_in? %>
      <tr>
        <td><%= current_user.name %></td>
        <td><%= current_user.email %></td>
        <td><%= current_user.admin %></td>
      </tr>
  <% end %>
</div>

</body>
</html> 