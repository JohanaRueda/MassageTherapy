    <thead>
      <tr>
        <th>#</th>
        <th>Course Name</th>
        <th>Course Description</th>
      </tr>
    </thead>
    <tbody>
      <% @courses.each do |course| %>
          <tr>
            <td><%= course.id %></td>
            <td><%= course.courseName %></td>
